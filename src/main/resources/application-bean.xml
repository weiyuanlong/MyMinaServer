    <!-- 操作gps的service层bean -->
    <bean id="gpsInfoService" class="com.aisafer.minasocket.service.impl.GPSInfoServiceImpl" />

    <!-- 操作用户的service层bean -->
    <bean id="userService" class="com.aisafer.minasocket.service.impl.UserServiceImpl" />

    <!-- 操作直播service层bean -->
    <bean id="seedingService" class="com.aisafer.minasocket.service.impl.SeedingServiceImpl" />

    <!-- 操作sim卡信息service层bean -->
